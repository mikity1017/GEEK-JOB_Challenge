package object333;

/**
 *
 * @author yokoi miki
 */

//クラスを作成
public class hensu{
    
    //publicな変数
    public static String hensu1 = "";
    public static int hensu2 = 0;
    
    //変数に値を入れるメソッド
    public  void meth1(String b){
        hensu1 = b;
    }
    
    public  void meth2(int a){
            hensu2 = a;
    }
    
    public  void pri(){
        System.out.println(hensu1+hensu2);
    }

}
